            <%= f.input :email, :required => true, name:"email", type:"text", placeholder:"E-mail", label:false %>
            <%= f.input :message, :as => :text, :required => true, type:"text", name:"message", placeholder:"Message", label:false, input_html: { class:"c-form-1-email form-control", style:"min-height:150px" } %>
